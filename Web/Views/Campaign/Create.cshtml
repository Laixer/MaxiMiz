@model Maximiz.InputModels.Campaign.CreateCampaignModel

@{
    ViewData["Title"] = "New";
}

@section Styles {
    <link href="~/css/stepwizard.css" />
}

<h1>New Campaign</h1>

<br />
<br />
<br />

<div class="container">
    <!--Step navigation buttons-->
    <div class="stepwizard">
        <div class="stepwizard-row row setup-panel">
            <div class="stepwizard-step col-md-2">
                <a href="#step-1" type="button" class="btn btn-primary btn-circle">
                    <img width="64" height="64" src="data:image/png;base64,">
                </a>
                <p><small>Account</small></p>
            </div>
            <div class="stepwizard-step col-md-2">
                <a href="#step-2" type="button" class="btn btn-default btn-circle" disabled="disabled">
                    <img width="64" height="64" src="data:image/png;base64,">
                </a>
                <p><small>Marketing</small></p>
            </div>
            <div class="stepwizard-step col-md-2">
                <a href="#step-3" type="button" class="btn btn-default btn-circle" disabled="disabled">
                    <img width="64" height="64" src="data:image/png;base64,">
                </a>
                <p><small>Targeting</small></p>
            </div>
            <div class="stepwizard-step col-md-2">
                <a href="#step-4" type="button" class="btn btn-default btn-circle" disabled="disabled">
                    <img width="64" height="64" src="data:image/png;base64,">
                </a>
                <p><small>Schedule</small></p>
            </div>
            <div class="stepwizard-step col-md-4">
                <a href="#step-5" type="button" class="btn btn-default btn-circle" disabled="disabled">
                    <img width="64" height="64" src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAADIAAAAyCAQAAAC0NkA6AAAAAmJLR0QA/4ePzL8AAABcSURBVFjD7dhBCgAgCERR73+tOVhtg2ghmJj92oXwQBmozLquEbRrIBHdAGmMyHQfOdW6EB0yoK1Wy2lFJKVdIJ8i5AQEhJwweBBy8ufgU271vLRA/EiPv5V31wSlrOuqMUPs9QAAAABJRU5ErkJggg==">
                </a>
                <p><small>Summary</small></p>
            </div>
        </div>
    </div>

    <form asp-controller="Campaign" asp-action="Create" method="post" role="form">
        <div class="panel panel-primary setup-content" id="step-1">
            <div class="panel-heading">
                <h2 class="panel-title">Account</h2>
            </div>
            <div class="panel-body">
                <div class="form-group">
                    <label class="control-label">Campaign</label>
                    <div class="row">
                        <img style="padding:8px; width:128px; height:48px;" src="https://www.taboola.com/sites/all/themes/taboola2017/images/taboola/logo-blue.png" />
                        <img style="padding:8px;  width:96px; border-radius:50%" src="data:image/jpeg;base64," />
                        <img style="padding:8px;  width:128px; height:48px;" src="https://www.google.com/images/branding/googlelogo/1x/googlelogo_color_272x92dp.png" />
                    </div>
                </div>
                <div class="form-group">
                    <label asp-for="Account" class="control-label"></label>
                    <input asp-for="Account" class="form-control" />
                    <span asp-validation-for="Account" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Name" class="control-label"></label>
                    <input asp-for="Name" class="form-control" />
                    <span asp-validation-for="Name" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="BrandingText" class="control-label"></label>
                    <input asp-for="BrandingText" class="form-control" />
                    <span asp-validation-for="BrandingText" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Url" class="control-label"></label>
                    <input asp-for="Url" class="form-control" />
                    <span asp-validation-for="Url" class="text-danger"></span>
                </div>
                <button class="btn btn-primary nextBtn pull-right" type="button">Next</button>
            </div>
        </div>

        <div class="panel panel-primary setup-content" id="step-2">
            <div class="panel-heading">
                <h2 class="panel-title">Marketing</h2>
            </div>
            <div class="panel-body">
                <div class="form-group">
                    <label asp-for="Objective" class="control-label"></label>
                    <input asp-for="Objective" class="form-control" />
                    <span asp-validation-for="Objective" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Objective" class="control-label"></label>
                    <input asp-for="AdDelivery" class="form-control" />
                    <span asp-validation-for="AdDelivery" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="AdDelivery" class="control-label"></label>
                    <input asp-for="SpendingLimit" class="form-control" />
                    <span asp-validation-for="SpendingLimit" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="SpendingLimit" class="control-label"></label>
                    <input asp-for="Cpc" class="form-control" />
                    <span asp-validation-for="Cpc" class="text-danger"></span>
                </div>
                <button class="btn btn-outline-primary prevBtn pull-left" type="button">Back</button>
                <button class="btn btn-primary nextBtn pull-right" type="button">Next</button>
            </div>
        </div>

        <div class="panel panel-primary setup-content" id="step-3">
            <div class="panel-heading">
                <h2 class="panel-title">Targeting</h2>
            </div>
            <div class="panel-body">
                <div class="form-group">
                    <label class="control-label">Geo-targeting</label>
                    <input maxlength="200" type="text" required="required" class="form-control" placeholder="" />
                </div>
                <div class="form-group">
                    <label class="control-label">Devices</label>
                    <input maxlength="200" type="text" required="required" class="form-control" placeholder="" />
                </div>
                <div class="form-group">
                    <label class="control-label">Operating system</label>
                    <input maxlength="200" type="text" required="required" class="form-control" placeholder="" />
                </div>
                <button class="btn btn-outline-primary prevBtn pull-left" type="button">Back</button>
                <button class="btn btn-primary nextBtn pull-right" type="button">Next</button>
            </div>
        </div>

        <div class="panel panel-primary setup-content" id="step-4">
            <div class="panel-heading">
                <h2 class="panel-title">Schedule</h2>
            </div>
            <div class="panel-body">
                <div class="form-group">
                    <label class="control-label">End Date</label>
                    <input type="date" required="required" class="form-control" placeholder="" />
                </div>
                <div class="form-group">
                    <label class="control-label">Start Date</label>
                    <input type="date" required="required" class="form-control" placeholder="" />
                </div>
                <button class="btn btn-outline-primary prevBtn pull-left" type="button">Back</button>
                <button class="btn btn-success pull-right nextBtn" type="button">Summary</button>
            </div>
        </div>

        <div class="panel panel-primary setup-content summary" id="step-5">
            <div class="panel-heading">
                <h2 class="panel-title">Summary</h2>
            </div>

            <h4>Account</h4>
            <table class="table">
                <tr>
                    <td>Account</td>
                    <td><span id="summary_account"></span></td>
                </tr>
                <tr>
                    <td>Name</td>
                    <td><span id="summary_name"></span></td>
                </tr>
                <tr>
                    <td>Branding</td>
                    <td><span id="summary_branding"></span></td>
                </tr>
                <tr>
                    <td>URL</td>
                    <td><span id="summary_url"></span></td>
                </tr>
            </table>

            <h4>Marketing</h4>
            <table class="table">
                <tr>
                    <td>Objective</td>
                    <td><span id="summary_objective"></span></td>
                </tr>
                <tr>
                    <td>CPC</td>
                    <td><span id="summary_cpc"></span></td>
                </tr>
                <tr>
                    <td>Spending Limit</td>
                    <td><span id="summary_spendingLimit"></span></td>
                </tr>
                <tr>
                    <td>Ad delivery</td>
                    <td><span id="summary_adDelivery"></span></td>
                </tr>
            </table>

            <button class="btn btn-outline-primary prevBtn pull-left" type="button">Back</button>
            <button class="btn btn-primary pull-right" type="submit">Create</button>
        </div>
    </form>
</div>

@section Scripts {
    <script>
        $(document).ready(function () {

            var navListItems = $('div.setup-panel div a'),
                allWells = $('.setup-content'),
                allNextBtn = $('.nextBtn'),
                allPrevBtn = $('.prevBtn'),
                summaryBtn = $('#summaryBtn');

            // Initially hide all wells
            allWells.hide();

            // Clicking on the navigation buttons
            navListItems.click(function (e) {
                e.preventDefault();
                var $target = $($(this).attr('href')),
                    $item = $(this);

                if ($target.hasClass("summary")) {
                    //TODO: Inject summary content
                    var account = $("#@Html.IdFor(m => m.Account)").val();
                    var name = $("#@Html.IdFor(m => m.Name)").val();
                    var branding = $("#@Html.IdFor(m => m.BrandingText)").val();
                    var url = $("#@Html.IdFor(m => m.Url)").val();

                    var objective = $("#@Html.IdFor(m => m.Objective)").val();
                    var adDelivery = $("#@Html.IdFor(m => m.AdDelivery)").val();
                    var spendingLimit = $("#@Html.IdFor(m => m.SpendingLimit)").val();
                    var cpc = $("#@Html.IdFor(m => m.Cpc)").val();

                    $("#summary_account").html(account);
                    $("#summary_name").html(name);
                    $("#summary_branding").html(branding);
                    $("#summary_url").html(url);

                    $("#summary_objective").html(objective);
                    $("#summary_adDelivery").html(adDelivery);
                    $("#summary_spendingLimit").html(spendingLimit);
                    $("#summary_cpc").html(cpc);
                }

                if (!$item.hasClass('disabled')) {
                    navListItems.removeClass('btn-primary').addClass('btn-default');
                    $item.addClass('btn-primary');
                    allWells.hide();
                    $target.show();
                    $target.find('input:eq(0)').focus();
                }
            });

            // Next step
            allNextBtn.click(function () {
                var curStep = $(this).closest(".setup-content"),
                    curStepBtn = curStep.attr("id"),
                    nextStepWizard = $('div.setup-panel div a[href="#' + curStepBtn + '"]').parent().next().children("a");

                nextStepWizard.removeAttr('disabled').trigger('click');
            });

            // Previous step (back)
            allPrevBtn.click(function () {
                var curStep = $(this).closest(".setup-content"),
                    curStepBtn = curStep.attr("id"),
                    prevStepWizard = $('div.setup-panel div a[href="#' + curStepBtn + '"]').parent().prev().children("a");

                prevStepWizard.removeAttr('disabled').trigger('click');
            });

            // Initial step click
            $('div.setup-panel div a.btn-primary').trigger('click');
        });
    </script>
}