@*
    Account populated fields.
*@

@model SettingsAccountViewModel

<div class="form-row">
    <div class="form-group col-md-6">
        <label for="inputFirstName">First Name</label>
        <input type="text" class="form-control" id="inputFirstName" disabled value="@Model.FirstName">
    </div>
    <div class="form-group col-md-6">
        <label for="inputLastName">Last Name</label>
        <input type="text" class="form-control" id="inputLastName" disabled value="@Model.LastName">
    </div>
</div>
<div class="form-row mt-4">
    <div class="form-group col-md-6">
        <label for="inputUsername">Username</label>
        <input type="text" class="form-control" id="inputUsername" disabled value="@Model.Email">
    </div>
    <div class="form-group col-md-6">
        <label for="inputdEmail">Email</label>
        <input type="email" class="form-control" id="inputEmail" disabled value="@Model.Email">
    </div>
</div>
